# === SCE: Structure Context (PART A/B 공통) ===
STRUCT_ENABLE=1
STRUCT_ATR_LEN=14
STRUCT_NEAR_THR_ATR=0.8
STRUCT_MAX_LEVELS=6

# price scale mode for structure overlays: auto|linear|log (default log)
STRUCT_SCALE_MODE=log
# y-axis chart scale: auto|linear|log
STRUCT_YAXIS_SCALE=auto
# fib channel base mode: recent|global|manual
STRUCT_FIB_BASE_MODE=recent
STRUCT_FIB_BASE=
STRUCT_FIB_BASE_KIND=bull
# near fib bands around price (0=all, intraday~1, daily~2)
STRUCT_FIB_TOPN_NEAR=1
# ATH lines
STRUCT_DRAW_ATH=1
STRUCT_DRAW_ATH_H=1
STRUCT_DRAW_ATH_V=1
STRUCT_COL_ATH=#000000
STRUCT_LW_ATH=1.3
STRUCT_ATH_ALPHA=0.65

# prev cycle tops
STRUCT_PREV_TOP_N=3
STRUCT_COL_PREV_TOP=#ff8c00
STRUCT_LW_PREV_TOP=1.2
STRUCT_PREV_TOP_SHOW_V=0
# trendline options
STRUCT_PIVOT_WINDOW=3
STRUCT_TL_USE_CLOSED=1
STRUCT_TL_ANCHOR_UP=low
STRUCT_TL_ANCHOR_DOWN=high
# x-axis tweaks
STRUCT_XTICK_MAX=12
STRUCT_XTICK_ROT=0
STRUCT_XGRID_ON=1
# ATH vertical line toggle
STRUCT_SHOW_ATH_V=0

# label toggles
STRUCT_LABELS_ON=1
# legend labels
STRUCT_LBL_TL_UP=Trend ↑
STRUCT_LBL_TL_DN=Trend ↓
STRUCT_LBL_REG=Regression μ
STRUCT_LBL_FIB_BASE=Fib base
STRUCT_LBL_FIB_LVL=Fib levels
STRUCT_LBL_FIB_MID=Fib mid

# (B에서 사용)
STRUCT_BREAK_CLOSE_ATR=0.2
